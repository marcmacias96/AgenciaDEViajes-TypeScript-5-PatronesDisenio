<center>
    <h1 style="background-color:DodgerBlue;">Tours</h1>
    
</center>

<div class="row" class="center">
    {{#each tours}}
    <div class="col-md-3">
        <div class="card">
            <div class="card-footer">
                <h2>{{name}}</h2>
            </div>
            <div class="card-body">
                <h1>{{ubication}}</h1>
                <p>{{description}}</p>
                <h2>{{pkcPrice}}</h2>
            </div>
            <div class="card-footer d-flex justify-content-between alig-items-center">
                <a href="/admin/tour/delete/{{_id}}" class="btn btn-danger">
                    Delete
                </a>
              
            </div>
        </div>
    </div>
    {{/each}}
</div>

    